{
    "name": "tuti-cli/cli",
    "description": "The Tuti CLI application.",
    "keywords": ["laravel zero", "console", "cli", "dev-ops", "headless", "tuti"],
    "homepage": "https://tuti.cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tuti-cli/cli/issues",
        "source": "https://github.com/tuti-cli/cli"
    },
    "authors": [
        {
            "name": "Yevhenii Volosiuk",
            "email": "volosiukweb@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/database": "^12.17",
        "laravel-zero/framework": "^12.0.2",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["tuti"]
}
